	<FontFamily x:Key="MTCMediaFontFamily">ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
	<FontFamily x:Key="PhoneFontFamilyNormal">ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
	<FontFamily x:Key="PhoneFontFamilySemiLight">ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
	<FontFamily x:Key="PivotHeaderItemFontFamily">ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
	<FontFamily x:Key="PivotTitleFontFamily">ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
	<FontFamily x:Key="ContentControlThemeFontFamily">ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
	<FontFamily x:Key="KeyTipFontFamily" >ms-appx:///Content/CnvIcons-Regular-SVG.ttf#CnvIcons,Segoe UI Variable,Segoe UI*,Segoe MDL2 Assets</FontFamily>
