<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <x:String x:Key="font">"Tw Cen MT Condensed"</x:String>
	<x:Double x:Key="GridRowHeight">34</x:Double>
	<x:Double x:Key="ShortGridRowHeight">24</x:Double>
	<x:Double x:Key="LargeFontSize">20</x:Double>
    <x:Double x:Key="MediumFontSize">12</x:Double>
    <x:Double x:Key="SmallFontSize">11</x:Double>
    <Style TargetType="Control">
        <Setter Property="IsTextScaleFactorEnabled" Value="False"/>
    </Style>
    <Style TargetType="ContentPresenter">
        <Setter Property="IsTextScaleFactorEnabled" Value="False"/>
    </Style>
	<x:String x:Key="fontStretch">Condensed</x:String>
</ResourceDictionary>
