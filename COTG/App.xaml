<Application x:Class="COTG.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Windows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract, 7)"
    xmlns:BelowWindows10version1809="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractNotPresent(Windows.Foundation.UniversalApiContract, 7)">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Microsoft.UI.Xaml/DensityStyles/Compact.xaml" />
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="/Styles/_Colors.xaml" />
                <ResourceDictionary Source="/Styles/_FontSizes.xaml" />
                <ResourceDictionary Source="/Styles/_Thickness.xaml" />
                <ResourceDictionary Source="/Styles/TextBlock.xaml" />
                <ResourceDictionary Source="/Styles/CommandBar.xaml" />
                <ResourceDictionary Source="/Styles/Page.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <ResourceDictionary.ThemeDictionaries>
                    <ResourceDictionary x:Key="Default">
                        <ResourceDictionary.MergedDictionaries>
                        <Windows10version1809:ColorPaletteResources Accent="#FF5C41FF" AltHigh="#FF000000" AltLow="#FF000000" AltMedium="#FF000000" AltMediumHigh="#FF000000" AltMediumLow="#FF000000" BaseHigh="#FFFFFFFF" BaseLow="#FF4D2B3A" BaseMedium="#FFAF939F" BaseMediumHigh="#FFC7ADB8" BaseMediumLow="#FF7E5F6C" ChromeAltLow="#FFC7ADB8" ChromeBlackHigh="#FF000000" ChromeBlackLow="#FFC7ADB8" ChromeBlackMedium="#FF000000" ChromeBlackMediumLow="#FF000000" ChromeDisabledHigh="#FF4D2B3A" ChromeDisabledLow="#FFAF939F" ChromeGray="#FF967986" ChromeHigh="#FF967986" ChromeLow="#FF31091D" ChromeMedium="#FF381124" ChromeMediumLow="#FF462233" ChromeWhite="#FFFFFFFF" ListLow="#FF381124" ListMedium="#FF4D2B3A" />
                            <ResourceDictionary>
                                <Color x:Key="SystemChromeAltMediumHighColor">#CC000000</Color>
                                <Color x:Key="SystemChromeAltHighColor">#FF4D2B3A</Color>
                                <Color x:Key="SystemAccentColor">#FF308BA8</Color>
                            <!-- Override system shape defaults -->
                                <!-- Override system borders -->
                                <!-- Override system generated accent colors -->
                        </ResourceDictionary>
                        </ResourceDictionary.MergedDictionaries>
                    </ResourceDictionary>
                    <ResourceDictionary x:Key="Light">
                        <ResourceDictionary.MergedDictionaries>
                            <Windows10version1809:ColorPaletteResources Accent="#FF62A38C" AltHigh="#FFFFFFFF" AltLow="#FFFFFFFF" AltMedium="#FFFFFFFF" AltMediumHigh="#FFFFFFFF" AltMediumLow="#FFFFFFFF" BaseHigh="#FF000000" BaseLow="#FFA4ACCC" BaseMedium="#FF5B709B" BaseMediumHigh="#FF2B487A" BaseMediumLow="#FF435C8A" ChromeAltLow="#FF2B487A" ChromeBlackHigh="#FF000000" ChromeBlackLow="#FFA4ACCC" ChromeBlackMedium="#FF2B487A" ChromeBlackMediumLow="#FF5B709B" ChromeDisabledHigh="#FFA4ACCC" ChromeDisabledLow="#FF5B709B" ChromeGray="#FF435C8A" ChromeHigh="#FFA4ACCC" ChromeLow="#FFD9E0F8" ChromeMedium="#FFCED6EF" ChromeMediumLow="#FFD9E0F8" ChromeWhite="#FFFFFFFF" ListLow="#FFCED6EF" ListMedium="#FFA4ACCC" />
                            <ResourceDictionary>
                                <Color x:Key="SystemChromeAltMediumHighColor">#CCD6DAEB</Color>
                                <Color x:Key="SystemChromeAltHighColor">#FFA4ACCC</Color>
                                <AcrylicBrush x:Key="SystemControlAcrylicWindowBrush" BackgroundSource="HostBackdrop" TintColor="{ThemeResource SystemChromeAltHighColor}" TintOpacity="0.8" FallbackColor="{ThemeResource SystemChromeMediumColor}" />
                                <!-- Override system shape defaults -->
                                <CornerRadius x:Key="ControlCornerRadius">0,2,2,0</CornerRadius>
                                <CornerRadius x:Key="OverlayCornerRadius">1,4,4,1</CornerRadius>
                                <!-- Override system borders -->
                                <!-- Override system generated accent colors -->
                                <Color x:Key="SystemAccentColorLight1">#FF73AF9A</Color>
                                <Color x:Key="SystemAccentColorLight2">#FF84BBA8</Color>
                                <Color x:Key="SystemAccentColorLight3">#FF96C8B6</Color>
                                <Color x:Key="SystemAccentColorDark1">#FF4E9179</Color>
                                <Color x:Key="SystemAccentColorDark2">#FF3B7F66</Color>
                                <Color x:Key="SystemAccentColorDark3">#FF276C52</Color>
                            </ResourceDictionary>
                        </ResourceDictionary.MergedDictionaries>
                    </ResourceDictionary>
                    <ResourceDictionary x:Key="HighContrast">
                        <StaticResource x:Key="RegionColor" ResourceKey="SystemColorWindowColor" />
                        <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
                    </ResourceDictionary>
                </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
