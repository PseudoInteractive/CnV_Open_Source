<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows10.0.20348.0</TargetFramework>
    <RunAnalyzersDuringBuild>false</RunAnalyzersDuringBuild>
    <OutputType>Library</OutputType>
    <Platforms>x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="node_modules\**" />
    <Compile Remove="Properties\**" />
    <Compile Remove="Ref\**" />
    <EmbeddedResource Remove="node_modules\**" />
    <EmbeddedResource Remove="Properties\**" />
    <EmbeddedResource Remove="Ref\**" />
    <None Remove="node_modules\**" />
    <None Remove="Properties\**" />
    <None Remove="Ref\**" />
    <TypeScriptCompile Remove="node_modules\**" />
    <TypeScriptCompile Remove="Properties\**" />
    <TypeScriptCompile Remove="Ref\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="ts\game122.js" />
  </ItemGroup>

  <ItemGroup>
    <TypeScriptCompile Remove="ts\gfunky3.user.ts" />
    <TypeScriptCompile Remove="ts\incoming-filter.user.ts" />
    <TypeScriptCompile Remove="ts\layoutPrint.ts" />
    <TypeScriptCompile Remove="ts\mail-city-coords.user.ts" />
  </ItemGroup>

  <ItemGroup>
    <Page Include="ts\mail-city-coords.user.ts" />
  </ItemGroup>

  <ItemGroup>
    <None Include="ts\gfunky3.user.ts" />
    <None Include="ts\incoming-filter.user.ts" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.DependencyValidation.Analyzers" Version="0.11.0" />
    <PackageReference Include="Microsoft.TypeScript.MSBuild" Version="4.5.1-rc">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>


  <PropertyGroup>
    <TypeScriptTarget>ES2015</TypeScriptTarget>
    <TypeScriptJSXEmit>None</TypeScriptJSXEmit>
    <TypeScriptModuleKind>None</TypeScriptModuleKind>
    <TypeScriptCompileOnSaveEnabled>False</TypeScriptCompileOnSaveEnabled>
    <TypeScriptNoImplicitAny>False</TypeScriptNoImplicitAny>
    <TypeScriptRemoveComments>True</TypeScriptRemoveComments>
    <TypeScriptOutFile>../COTG/JS/funky</TypeScriptOutFile>
    <TypeScriptOutDir />
    <TypeScriptGeneratesDeclarations>True</TypeScriptGeneratesDeclarations>
    <TypeScriptNoEmitOnError>False</TypeScriptNoEmitOnError>
    <TypeScriptSourceMap>True</TypeScriptSourceMap>
    <TypeScriptMapRoot />
    <TypeScriptSourceRoot />
  </PropertyGroup>


  <PropertyGroup>
    <DebugType>portable</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <DefineConstants></DefineConstants>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Optimize>true</Optimize>
  </PropertyGroup>


  <PropertyGroup>
    <PlatformTarget>x64</PlatformTarget>
    <IsPublishable>False</IsPublishable>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
  </PropertyGroup>


  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>


  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>



</Project>
